{
    "name": "dyrynda/laravel-model-uuid",
    "description": "This package allows you to easily work with UUIDs in your Laravel models.",
    "license": "MIT",
    "keywords": [
        "eloquent",
        "laravel",
        "model",
        "uuid"
    ],
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au",
            "homepage": "https://dyrynda.com.au"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.12|^9.0",
        "illuminate/events": "^8.12|^9.0",
        "illuminate/support": "^8.12|^9.0",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Dyrynda\\Database\\Support\\": "src/"
        }
    },
    "require-dev": {
        "dyrynda/laravel-efficient-uuid": "^4.5.2",
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "dyrynda/laravel-efficient-uuid": "Override Laravel's default query grammar to efficiently store UUIDs."
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "6.x-dev"
        }
    }
}
